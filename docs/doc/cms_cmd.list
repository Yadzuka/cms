#cmd_list	cmpltn	d	opts	access	name	desc	comment (cmd_list - выдает этот список команд)
ls	+	d	-	:,read	Содержимое	Просмотр содержимого директории или мета-данных документа	
view	-	d	-	:,read	Просмотр	Просмотр документа или директории		
get	--	d	-	:,read	HTTP/GET	Просмотр документа или директории по http
post	---	d	-	:	HTTP/POST	Просмотр документа или директории по http
exec	---	d,d2	-	:	Исполнить	Исполнить скрипт на сервере
lexec	---	d,d2	-	:	Исполнить локально	Исполнить скрипт на клиенте
mv	-	d,d2	-	:,create:d2	Переместить	переместить документ в другую директорию	
rename	-	d,d2	-	:,	переименовать	переименовать по месту		
mkdir	-	d	-	:,	Создать директорию	Создать новую директорию		
rmdir	--	d	-	:,	Удалить директорию	Удалить директорию		
sln	---	d,d2	-a	:,	ln -s d2 d	создать символическую ссылку на файл d2 (-a абсолютную)
rm	-	d	-	:,	Удалить документ	Удалить документ
unrm	---	d,d2	-	:,	Восстановть	Восстановить удаленный документ		
restore	---	d,d2	-	:,	Восстановть версию	Восстановить версию документа		
cp	-	d,d2	-	:,	Копировать	Копировать документ под новым именем		
ci	--	d,d2	-	:,	Сохранить версию	(внутренняя, а-ля RCS ci) поместить версию в репозиторий
co	--	d,d2	-	:,	Восстановить версию	(внутренняя, а-ля RCS co) извлечь версию из репозитория и сделать актуальной
edit	-	d	-	:,	Редактировать	Редактировать документ (выбор способа и управление процессом)		
begin	-------	d	-	:,	Начать транзакцию	Начать комплексную транзакцию для редактирования всех документов в d
lock	--	d	-	:,	Заблокировать	Заблокировать документ на редактирование другими пользователями		
unlock	--	d	-	:,	Разблокиовать	Разблокировать документ		
create	+-	d	-	:,	создать	создать пустой документ и перейти к его редактированию		
open	-	d	-m,	:,	Открыть	Открыть документ на редактирование способом --mode (начать редактирование)		
write	+-	d	-	:,	Записать	Записать новое содержимое в редактируемый файл
wread	--	d	-m	:,	прочитать записанное	Прочитать файл, его временную записанную копию, как текстовый, в заданной кодировке (или по-умолчанию), используя -m mime-type
qc	--	d	-	:,	Контроль качества	применить контроль качества к редактируемому документу		
commit	-	d	-	:,	Применить изменения	применить изменения в редактируемом документе и закрыть его (завершть редактирование).		
rollbck	-	d	-	:,	Отменить изменения	Отменить изменения в редактируемом документе и закрыть его (завершть редактирование).		
fetch	---	d,d2	-	:,	fetch	загрузить на место файла d файл из сети по url d2
chown	----	d	-g	:,	chown	изменить [группу] владельца файла (-g group)
chmod	----	d	-g	:,	chmod	изменить права POSIX права доступа на документ
set	----	d,d2	-g	:,	set	установить мета-параметры документа
upload	+-	d	-	:,	загрузить	загрузить файлы в указанную директорию или файл, вместо указанного файла
downld	+	d	-m	:,	выгрузить	Выгрузить файл, как бинарный, используя -m mime-type
read	--	d	-m	:,	прочитать	Прочитать файл, как текстовый, в заданной кодировке (или по-умолчанию), используя -m mime-type
insert	--	d,d2	-	:,	вставить запись	SQL/CSV/PSPN insert row[s], d2 - запись в csv формате, начинается с ; или выражение		
update	--	d,d2	-	:,	обновить запись	SQL/CSV/PSNP update row[s], d2 - запись в csv формате		
delete	--	d,d2	-	:,	удалить запись	SQL/CSV/PSPN delete row[s], d2 - запись в csv формате		
select	--	d,d2	-	:,	выбрать записи	SQL/CSV/PSPN select row[s], d2 - запись в csv формате, 		
